{
    "name": "jackmartin/laravel-yandex-geocode",
    "description": "Simply service laravel Yandex Geocoding",
    "keywords": [
        "laravel",
        "yandex",
        "geocode",
        "geocoding",
        "address",
        "latitude",
        "longitude",
        "api"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Evgen Kytonin",
            "email": "killfess@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Yandex\\Geo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yandex\\Geo\\YandexServiceProvider"
            ],
            "aliases": {
                "YaGeo": "Yandex\\Geo\\Facades\\YaGeo"
            }
        }
    }
}